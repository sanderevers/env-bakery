{
  "name": "env-bakery",
  "version": "0.0.0",
  "license": "BSD-2-Clause",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "node copy-assets.js",
    "build": "ng build lib --configuration production",
    "postbuild": "npm run build:schematics",
    "watch": "ng build lib --watch",
    "test": "ng test lib",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "publish": "npm publish dist/lib",
    "parbake": "node projects/lib/assets/bin/parbake.js",
    "format": "dprint fmt",
    "format:check": "dprint check",
    "prepare": "husky install",
    "build:schematics": "tsc -p projects/lib/tsconfig.schematics.json",
    "copy:schemas": "cpy --cwd=projects/lib --parents schematics/*/schema.json ../../dist/lib",
    "copy:files": "cpy --cwd=projects/lib --parents schematics/*/files/** ../../dist/lib/",
    "copy:collection": "cpy --cwd=projects/lib schematics/collection.json ../../dist/lib/schematics",
    "postbuild:schematics": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.11",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.11",
    "@angular/core": "~13.3.11",
    "@angular/forms": "~13.3.11",
    "@angular/platform-browser": "~13.3.11",
    "@angular/platform-browser-dynamic": "~13.3.11",
    "@angular/router": "~13.3.11",
    "dotenv": "^10.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.7",
    "@angular/cli": "~13.3.7",
    "@angular/compiler-cli": "~13.3.11",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "cpy-cli": "^3.1.1",
    "dprint": "^0.17.0",
    "husky": "^7.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}